{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "no-unused-vars":["error",{"vars": "all","args":"after-used","ignoreRestSiblings":false,"argsIgnorePattern":"React|^_","caughtErrors":"all","caughtErrorsIgnorePattern":"^ignore"}],
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error"
    }
}
